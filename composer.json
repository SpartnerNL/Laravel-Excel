{
  "name": "seoperin/laravel-excel",
  "description": "Supercharged Excel exports and imports in Laravel",
  "license": "MIT",
  "keywords": [
    "laravel",
    "php",
    "phpspreadsheet",
    "phpexcel",
    "excel",
    "csv",
    "export",
    "import",
    "batch"
  ],
  "authors": [
    {
      "name": "Evgeniy Perin",
      "email": "seoperin@gmail.com"
    }
  ],
  "require": {
    "ext-json": "*",
    "php": "^7.0",
    "phpoffice/phpspreadsheet": "^1.6",
    "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.*",
    "opis/closure": "^3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "mockery/mockery": "^1.1",
    "orchestra/testbench": "^3.8",
    "orchestra/database": "^3.8",
    "predis/predis": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Seoperin\\LaravelExcel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Seoperin\\LaravelExcel\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Seoperin\\LaravelExcel\\ExcelServiceProvider"
      ],
      "aliases": {
        "Excel": "Seoperin\\LaravelExcel\\Facades\\Excel"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
